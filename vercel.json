{
  "version": 2,
  "builds": [
    {
      "src": "sistema_escolar_api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sistema_escolar_api/wsgi.py"
    }
  ]
}